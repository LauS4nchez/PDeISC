            <button id="desplegarNumeros" class="btn btn-primary mt-3">Mostrar Numeros y Eliminar el primero</button>
            <div id="eliminadoNum" class="mt-3">

            </div>
            <div id="numeros" class="mt-3">

            </div>
        </div>
    </div>
    
    <!-- Botón para los mensajes -->
    <div class="card mb-4">
        <div class="card-body">
            <h3>Eliminar primer mensaje</h3>
            <button id="desplegarMensajes" class="btn btn-primary mt-3">Mostrar Mensajes y Eliminar el primero</button>
            <div id="eliminadoMsg" class="mt-3">

            </div>
            <div id="mensajes" class="mt-3">

            </div>
        </div>
    </div>

    <!-- Fila de compras -->
    <div class="card mb-4">
        <div class="card-body">
            <h3>Fila de Compras</h3>
            <button id="desplegarFila" class="btn btn-primary mt-3">Mostrar Fila y Eliminar el primero</button>
            <div id="eliminadoFila" class="mt-3">

